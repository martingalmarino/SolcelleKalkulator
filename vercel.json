{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "rewrites": [
    {
      "source": "/solcelle-kalkulator",
      "destination": "/"
    },
    {
      "source": "/solcelle-kalkulator/:fylke",
      "destination": "/solcelle-kalkulator/:fylke"
    },
    {
      "source": "/solcelle-kalkulator/:fylke/:kommune",
      "destination": "/solcelle-kalkulator/:fylke/:kommune"
    }
  ]
}
